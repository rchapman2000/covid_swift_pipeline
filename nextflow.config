profiles {	
    standard {	
        process {	
            withName: 'GeneratePileup' {	
                cpus = 4
                memory = 16.GB	
            }
            withName: 'IvarConsensus' {	
                cpus = 4
                memory = 16.GB	
            }
        }   
    }
    cloud {	
        process {	
            withName: 'GeneratePileup' {	
                cpus = 8
                memory = 32.GB	
            }
            withName: 'IvarConsensus' {	
                cpus = 4
                memory = 16.GB	
            }
    }
    
    }	
// Docker always true. Containers defined in each process
docker.enabled = true
}
